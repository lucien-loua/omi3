{
  "name": "@omi3/utils",
  "description": "Reusable constants and utility functions",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf .turbo node_modules dist",
    "type-check": "tsc --noEmit",
    "lint": "biome check --write .",
    "test": "vitest run --passWithNoTests"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xyhomi3/omi3.git"
  },
  "homepage": "https://github.com/xyhomi3/omi3/tree/main/packages/utils#readme",
  "bugs": {
    "url": "https://github.com/xyhomi3/omi3/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@omi3/typescript": "workspace:*",
    "@types/node": "^22",
    "tsup": "^8.4.0",
    "typescript": "^5",
    "vitest": "^3.1.2"
  }
}