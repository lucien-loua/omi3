{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
